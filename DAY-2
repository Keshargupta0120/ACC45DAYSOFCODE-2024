#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T; 
    
    while (T--) {
        int N, X; 
        cin >> N >> X; 
        int subscriptions = (N + 5) / 6; // this ensures we round up for any remaining people
        int total_cost = subscriptions * X;
        cout << total_cost << endl;
    }
    
    return 0;
}
